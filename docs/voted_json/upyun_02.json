{
  "both": {
    "description": "固定宽度和高度，宽高不足时居中裁剪再缩放。特别地，配合 `/force/true` 使用时，宽高不足时只缩放，不裁剪",
    "pattern": "/both/{}x{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "宽x高，如 300×200"
  },
  "sq": {
    "description": "图片缩放成正方形，宽高相等",
    "pattern": "/sq/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "宽或高，如 300"
  },
  "crop": {
    "description": "缩小或放大前进行裁剪",
    "pattern": "/crop/{}x{}a{}a{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "宽x高axay，如 300x200a80a60"
  },
  "fwfh": {
    "description": "限定宽度或高度，宽高不足时不缩放",
    "pattern": "/fwfh/{}x{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "宽x高，如 300×200"
  },
  "fxfn2": {
    "description": "限定长边最小值和短边最小值，进行等比缩放，不裁剪",
    "pattern": "/fxfn2/{}x{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "长边x短边，如 300×200"
  },
  "max": {
    "description": "限定最长边，短边自适应",
    "pattern": "/max/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "最长边，如 200"
  },
  "roundrect": {
    "description": "裁剪时对四角进行圆化（圆角裁剪）",
    "pattern": "/roundrect/{}",
    "separator": "/",
    "type": "integer",
    "default": "10",
    "constraints": "圆角半径，如 20"
  },
  "fwfh2": {
    "description": "限定宽度最小值和高度最小值，宽高不足时不缩放",
    "pattern": "/fwfh2/{}x{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "宽x高，如 300×200"
  },
  "min": {
    "description": "限定最短边，长边自适应",
    "pattern": "/min/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "最短边，如 200"
  },
  "fw": {
    "description": "限定宽度，高度自适应",
    "pattern": "/fw/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "宽，如 300"
  },
  "fh": {
    "description": "限定高度，宽度自适应",
    "pattern": "/fh/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "高，如 200"
  },
  "fp": {
    "description": "宽高等比例缩放，直到宽高像素积小于但最接近指定值",
    "pattern": "/fp/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "宽高像素积，如 200000，取值范围 [1-25000000]"
  },
  "fxfn": {
    "description": "限定长边或短边，进行等比缩放，不裁剪",
    "pattern": "/fxfn/{}x{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "长边x短边，如 300×200"
  },
  "force": {
    "description": "不支持放大的参数，指定 `/force/` 为 `true` 进行放大，默认 `false`",
    "pattern": "/force/{}",
    "separator": "/",
    "type": "boolean",
    "default": "false",
    "constraints": "true"
  },
  "gravity": {
    "description": "裁剪开始的方位，默认 `northwest`，详见[方位说明](#align_gravity)。特别地，`/gravity` 需要放在 `/crop` 或 `/clip` 的后面",
    "pattern": "/gravity/{}",
    "separator": "/",
    "type": "string",
    "default": "northwest",
    "constraints": "位置，如 north"
  },
  "scale": {
    "description": "宽高等比例缩放",
    "pattern": "/scale/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "缩放比例，如 50，取值范围 [1-1000]"
  },
  "clip": {
    "description": "缩小或放大后进行裁剪",
    "pattern": "/clip/{}x{}a{}a{}",
    "separator": "/",
    "type": "string",
    "default": "null",
    "constraints": "宽x高axay，如 300x200a80a60"
  },
  "wscale": {
    "description": "宽度按比例缩放，高度不变",
    "pattern": "/wscale/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "宽度缩放比例，如 200，取值范围 [1-1000]"
  },
  "hscale": {
    "description": "高度按比例缩放，宽度不变",
    "pattern": "/hscale/{}",
    "separator": "/",
    "type": "integer",
    "default": "null",
    "constraints": "高度缩放比例，如 200，取值范围 [1-1000]"
  }
}